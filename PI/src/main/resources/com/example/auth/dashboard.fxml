<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.auth.controller.DashboardController">
    <Text text="Dashboard" style="-fx-font: bold 24px 'System';"/>
    <Label fx:id="welcomeLabel" text="Welcome, User!" style="-fx-font: 18px 'System';"/>
    <Label fx:id="emailLabel" text="Email: " style="-fx-font: 14px 'System';"/>
    <Button text="View Reclamations" onAction="#handleReclamationLink" style="-fx-background-color: #6C983B; -fx-text-fill: white; -fx-font-size: 14px; -fx-padding: 10; -fx-background-radius: 5;"/>
    <Button text="Logout" onAction="#handleLogout" style="-fx-background-color: #ff5555; -fx-text-fill: white; -fx-font-size: 14px; -fx-padding: 10; -fx-background-radius: 5;"/>
</VBox>